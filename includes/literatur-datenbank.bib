@article{limSoftwareComplexityEstimation2010,
  title = {The {{Software Complexity Estimation Method}} in {{Algorithm Level}} by {{Analysis}} of {{Source}} Code},
  author = {Lim, Woong and Nam, Jung-Hak and Sim, D. and Cho, Dae-Sung and Choi, W.},
  date = {2010},
  journaltitle = {undefined},
